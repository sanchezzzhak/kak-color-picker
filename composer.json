{
    "name": "kak/colorpicker",
    "description": "color picker widgets for Yii2",
    "type": "library",
    "keywords": ["yii2","color picker yii2","kak color picker"],
    "license": "MIT",
    "support": {
      "issues": "https://github.com/sanchezzzhak/kak-color-picker/issues?state=open",
      "source": "https://github.com/sanchezzzhak/kak-color-picker"
    },
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
    "require": {
        "simonwep/pickr": "1.5.1"
    },
    "autoload": {
        "psr-4": {
            "kak\\widgets\\colorpicker\\": "src"
        }
    },
     "repositories": [
        {
           "type": "package",
           "package": {
                "name": "simonwep/pickr",
                "version": "1.5.1",
                "dist": {
                    "url": "https://github.com/Simonwep/pickr/archive/1.5.1.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/Simonwep/pickr.git",
                    "type": "git"
                }
            }  
        }
    ]
}
